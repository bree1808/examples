//2 point formula
#include <stdio.h>

int main()
 {
     float x1, y1;
     float x2, y2;
     float x, y;
     
     printf("Enter points for x1, y1, x2, y2, x: ");
     scanf("%f %f %f %f %f", &x1, &y1, &x2, &y2, &x);
        y = (y2 - y1) / (x2 - x1) * (x - x1) + y1 ;
            printf("y = %f\n", y);
    return 0;
 }