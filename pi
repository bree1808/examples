//Implement the Ï€ calculation algorithm talked about during lectures. The pseudocode is:
#include <stdio.h>

int main()
{
    int countTotal = 0
    int countInside = 0
    int x, y;
    
while (countTotal < A large number)
{
    x = random number between 0 and 1
    y = random number between 0 and 1
    countTotal = countTotal + 1;
    if ( x*x + y*y < 1)
    {
        countInside = countInside + 1;
    }
}
pi = 4*countInside/countTotal;
printf("%d\n", pi);
}